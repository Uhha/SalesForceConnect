                                    CanHorizontallyScroll="False" CanVerticallyScroll="False" 
                                    ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" 
                                    Grid.Column="0" Margin="{TemplateBinding Padding}" Grid.Row="0"/>
