<UserControl x:Class="SFConnectorFullScreenUI.LogBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SFConnectorFullScreenUI"
             mc:Ignorable="d" 
             Height="60"
             >
    <Grid >
        <TextBox x:Name="log" BorderThickness="0" 
                 Background="{StaticResource BackgroundBrush}"
                 Foreground="{StaticResource ForegroundFiftyBrush}"
                 Text="Logger is Ready">
            
        </TextBox>
    </Grid>
</UserControl>
