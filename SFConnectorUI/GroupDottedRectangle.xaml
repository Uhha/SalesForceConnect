<UserControl x:Class="SFConnectorUI.GroupDottedRectangle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SFConnectorUI"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="50"
             Canvas.Left="0"
             Canvas.Top="0"
             >
    <Grid>
        <Label x:Name="label" Content="Title" Foreground="White" Margin="-5,-21,0,0" />
        <Rectangle Stroke="White" StrokeThickness="1"  StrokeDashArray="4 4" />
        
    </Grid>
</UserControl>
